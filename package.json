    "babel-preset-stage-2": "^6.24.1",
    "concurrently": "^3.4.0",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.4",
    "file-loader": "^0.11.2",
    "gulp": "^3.9.1",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "node-sass": "^4.5.3",
