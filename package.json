{
  "name": "ftk-vue",
  "private": true,
  "version": "0.0.0",
  "description": "a Sails application",
  "keywords": [],
  "dependencies": {
    "ejs": "2.3.4",
    "grunt": "1.0.1",
    "grunt-contrib-clean": "1.0.0",
    "grunt-contrib-coffee": "1.0.0",
    "grunt-contrib-concat": "1.0.1",
    "grunt-contrib-copy": "1.0.0",
    "grunt-contrib-cssmin": "1.0.1",
    "grunt-contrib-jst": "1.0.0",
    "grunt-contrib-less": "1.3.0",
    "grunt-contrib-uglify": "1.0.1",
    "grunt-contrib-watch": "1.0.0",
    "grunt-sails-linker": "~0.10.1",
    "grunt-sync": "0.5.2",
    "include-all": "^1.0.0",
    "rc": "1.0.1",
    "sails": "~0.12.13",
    "sails-disk": "~0.10.9"
  },
  "scripts": {
    "dev": "concurrently \"npm run semantic\" \"npm run vue\" \"npm start\"",
    "semantic": "cd build && gulp watch",
    "vue": "webpack-dev-server --config build/webpack.config.js --host=0.0.0.0 --hot --inline",
    "build": "npm run build-semantic && npm run build-vue",
    "build-semantic": "cd build/semantic && gulp build",
    "build-vue": "cross-env NODE_ENV=production webpack --config build/webpack.config.js --hide-modules",
    "debug": "node debug app.js",
    "start": "node app.js",
    "postinstall": "npm run build-semantic"
  },
  "main": "app.js",
  "repository": {
    "type": "git",
